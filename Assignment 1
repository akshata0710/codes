#include<stdio.h>
int main()
{
	int num,a,rev=0,rem;
	printf("enter any number:");
	scanf("%d",&num);
	num=a;
	while(num>0)
	{
		rem=num%10;
		rev=rev*10+rem;
		num=num/10;
	}
	if(num==rev)
	{
		printf("Given number is a palindrome");
		
	}
	else
	{
		printf("Given number is not a palindrome");
	}
	return 0;
}